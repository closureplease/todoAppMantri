{
  "jsRoot": "./js/",
  "vendorLibs": {
    "yepnope": "vendor/yepnope/yepnope.1.5.4-min",
    "jquery": "../assets/jquery.min",
    "handlebars": "../assets/handlebars.min",
    "hbsCompiled": "app/templates/hbsCompiled",
    "ember": "vendor/ember-latest.min"
  },

  "build": {
    "src": "js/app.js",
    "dest": "dist/app.min.js",
    "outputWrapper": "var BUILD=true;%output%",
    "sourceMapFile": "dist/app.min.js.map",
    "sourceMappingURL": "/dist/app.min.js.map"
  },
  "buildModules": {
    "Todos.ctrls.Entries": {
      "dest": "dist/app-module-entries.min.js",
      "sourceMapFile": "dist/app-module-entries.min.js.map",
      "sourceMappingURL": "/dist/app-module-entries.min.js.map"
    },
    "Todos.views.Application": {
      "dest": "dist/app-module-application.min.js",
      "sourceMapFile": "dist/app-module-application.min.js.map",
      "sourceMappingURL": "/dist/app-module-application.min.js.map"
    }
  }
}
